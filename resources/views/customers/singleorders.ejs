<section class="status cart">
    <div class="container mx-auto statusbox">
        <div class="w-2/3 mx-auto ">
            <div class="flex items-center justify-between mb-12">
                <h1 class="text-xl font-bold">Track Status</h1>
                <input type="hidden" value="<%=JSON.stringify(order)%>" id="order">
                <h6 class="py-1 rounded-full bg-white px-4 text-xs"><%=order._id%></h6>


            </div>
            <ul>
                <li class="statusline pb-16 text-xl " data="Order_placed"><span>Order Placed</span></li>
                <li class="statusline pb-16 text-xl " data="confirmed"><span>Order Confirmation</span></li>
                <li class="statusline pb-16 text-xl " data="prepared"><span>Order Preparation</span></li>
                <li class="statusline pb-16 text-xl " data="delivered"><span>Delivered</span></li>
                <li class="statusline text-xl last " data="delivered"><span>Complete</span></li>
            </ul>
        </div>
    </div>
</section>